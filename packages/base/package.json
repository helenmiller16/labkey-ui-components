{
  "name": "@glass/base",
  "version": "0.22.0-fb-issue-37776.1",
  "description": "Base components, models, and utility functions for LabKey applications and pages",
  "main": "dist/base.cjs.js",
  "module": "dist/base.es.js",
  "types": "dist/index.d.ts",
  "config": {
    "application_dir": "/path/to/application/node_modules/\\@glass/base/dist"
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "nps build",
    "clean": "nps clean",
    "cleanAll": "nps cleanAll",
    "prepare": "yarn build",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "build-storybook": "build-storybook -c .storybook -o .storybook-out",
    "test": "cross-env NODE_ENV=test jest",
    "test_teamcity": "cross-env NODE_ENV=test jest --testResultsProcessor=jest-teamcity-reporter && yarn run build-storybook",
    "watch:build": "watch 'yarn build' ./src",
    "watch:distCopy": "cpx --watch -v './dist/**/*' $npm_package_config_application_dir",
    "watch:buildAndCopy": "npm run watch:build & npm run watch:distCopy",
    "watch": "npm run watch:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LabKey/glass-components.git"
  },
  "author": "Cory Nathe",
  "license": "SEE LICENSE IN LICENSE.txt",
  "bugs": {
    "url": "https://github.com/LabKey/glass-components/issues"
  },
  "publishConfig": {
    "registry": "https://artifactory.labkey.com/artifactory/api/npm/libs-client"
  },
  "homepage": "https://github.com/LabKey/glass-components#readme",
  "dependencies": {
    "@labkey/dependencies": "0.0.10",
    "bootstrap": "3.4.1",
    "moment": "2.24.0",
    "moment-jdateformatparser": "1.1.0",
    "moment-timezone": "^0.5.27",
    "numeral": "2.0.6"
  },
  "devDependencies": {
    "@types/numeral": "0.0.25",
    "bootstrap-sass": "3.4.1"
  },
  "peerDependencies": {
    "immutable": "^3.8.2",
    "react": "^16.0",
    "react-dom": "^16.0"
  },
  "jest": {
    "globals": {
      "LABKEY": {}
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "moduleNameMapper": {
      "\\.(scss)$": "<rootDir>/../../test/styleMock.ts"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/../../test/jest.setup.ts"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/"
    ],
    "testRegex": "(\\.(test|spec))\\.(ts|tsx)$",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    }
  },
  "gitHead": "4edd31c934e5bea492455ec2cedc7b083cff4ba5"
}
